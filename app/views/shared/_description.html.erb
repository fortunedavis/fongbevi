<div class="p-4 sm:p-6 md:p-8 bg-gray-100">
  <% @users.each do |user| %>
    <div class="border-b border-gray-300 mb-4 pb-4">
				<p class="flex flex-col items-center justify-center pb-4 mb-4"> Liste des participants</p>

		<%if user.clips.count()>1%>
      <div class="flex flex-col items-center justify-center">
        <div class="flex justify-center space-x-4">
				   <h2 class="text-xl font-bold mb-2"><%= user.fullname %> </h2>
        </div>
      </div>
		<% end %>	
    </div>
  <% end %>
</div>

<section class="wow fadeInDown md:flex mt-10 md:space-x-10 justify-center " data-wow-offset="10"  data-wow-iteration="10">
  	<div class="animate__animated animate__fadeInLeft flex flex-wrap flex-row">
        <div class="p-6 bg-white dark:bg-gray-800 rounded-lg shadow-lg mb-6">
          			<div class="flex flex-row justify-between pb-6">
								  <div class="flex flex-col">
		                <h3 class="text-base font-bold">Inscription des Utilisateurs</h3>
		              </div>
		            </div>
	              <div class="relative">
                  <%= line_chart User.group_by_day(:created_at).count, width: "400px", height: "200px"%>
	              </div>
							</div>
			  </div>
        <div class="animate__animated animate__fadeInRight">
          		<div class="p-6 bg-white dark:bg-gray-800 rounded-lg shadow-lg mb-6">
          			<div class="flex flex-row justify-between pb-6">
								  <div class="flex flex-col">
		                <h3 class="text-base font-bold">Fréquence des enrégistrements</h3>
		              </div>
		            </div>
	              <div class="relative">
                  <%= area_chart Clip.group_by_day(:created_at).count, width: "400px", height: "200px"  %>
	              </div>
							</div>
						</div>
        </div>     
</section>
